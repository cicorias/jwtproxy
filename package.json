{
  "name": "jwtproxy",
  "version": "1.0.3",
  "description": "simple jwt validation proxy for express",
  "main": "./build/index.js",
  "scripts": {
    "build": "tsc --build .",
    "test": "mocha ./tests",
    "preversion": "npm test",
    "version": "npm run build && git add -Af dist",
    "postversion": "git push && git push --tags && rm -rf build/temp"
  },
  "files": [
    "src/",
    "dist/",
    "tests/",
    ".gitignore",
    "CODE_OF_CONDUCT.md",
    "LICENSE",
    "package-lock.json",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cicorias/jwtproxy.git"
  },
  "keywords": [
    "jwt",
    "validation",
    "proxy"
  ],
  "author": "Shawn Cicoria <github@cicoria.com> (https://cicoria.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cicorias/jwtproxy/issues"
  },
  "homepage": "https://github.com/cicorias/jwtproxy#readme",
  "devDependencies": {
    "mocha": "^7.1.2",
    "supertest": "^4.0.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.0"
  }
}
