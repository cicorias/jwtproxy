{
  "name": "jwtproxy",
  "version": "1.0.4",
  "description": "simple jwt validation proxy for express",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc --build .",
    "dev:static": "ts-node ./src/server.ts",
    "dev": "nodemon",
    "test": "mocha -r ts-node/register -r mocha-clean tests/**/*.ts",
    "test:setup": "mocha -r ts-node/register -r mocha-clean tests/**/jwt_setup_tests.ts",
    "test:watch": "watch  -c -n 10 -t npm run test -- --reporter min",
    "preversion": "npm test",
    "version": "npm run build && git add -Af dist",
    "postversion": "git push && git push --tags && rm -rf build/temp"
  },
  "files": [
    "src/",
    "dist/",
    "tests/",
    ".gitignore",
    "CODE_OF_CONDUCT.md",
    "LICENSE",
    "package-lock.json",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cicorias/jwtproxy.git"
  },
  "keywords": [
    "jwt",
    "validation",
    "proxy"
  ],
  "author": "Shawn Cicoria <github@cicoria.com> (https://cicoria.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cicorias/jwtproxy/issues"
  },
  "homepage": "https://github.com/cicorias/jwtproxy#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/debug": "^4.1.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^7.0.2",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "chai": "^4.2.0",
    "colors": "^1.4.0",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "mocha": "^7.1.2",
    "mocha-clean": "^1.0.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.0"
  }
}
